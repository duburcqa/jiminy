<?xml version="1.0" ?>
<robot name="pendulum">
    <link name="Foot">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <geometry>
                <box size="0.2 0.2 0.01"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="2.0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
    </link>
    <joint name="Corner1Joint" type="fixed">
      <origin xyz="0.1 0.1 0.0" rpy="0.0 0.0 0.0"/>
      <parent link="Foot"/>
      <child link="Corner1"/>
    </joint>
    <link name="Corner1">
    </link>
    <joint name="Corner2Joint" type="fixed">
      <origin xyz="0.1 -0.1 0.0" rpy="0.0 0.0 0.0"/>
      <parent link="Foot"/>
      <child link="Corner2"/>
    </joint>
    <link name="Corner2">
    </link>
    <joint name="Corner3Joint" type="fixed">
      <origin xyz="-0.1 -0.1 0.0" rpy="0.0 0.0 0.0"/>
      <parent link="Foot"/>
      <child link="Corner3"/>
    </joint>
    <link name="Corner3">
    </link>
    <joint name="Corner4Joint" type="fixed">
      <origin xyz="-0.1 0.1 0.0" rpy="0.0 0.0 0.0"/>
      <parent link="Foot"/>
      <child link="Corner4"/>
    </joint>
    <link name="Corner4">
    </link>

    <joint name="PendulumJoint" type="revolute">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <axis xyz="1 0 0"/>
      <parent link="Foot"/>
      <child link="PendulumArm"/>
      <limit effort="1000" velocity="100" lower="-100" upper="100"/>
    </joint>

    <link name="PendulumArm">
        <visual>
            <origin xyz="0.0 0.0 0.5" rpy="0.0 0.0 0.0" />
            <geometry>
                <box size="0.01 0.01 1.0"/>
            </geometry>
            <material name="">
                <color rgba="0.0 0.0 1.0 1.0"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>

    <joint name="PendulumMassJoint" type="fixed">
      <origin xyz="0.0 0.0 1.0" rpy="0.0 0.0 0.0"/>
      <parent link="PendulumArm"/>
      <child link="PendulumMass"/>
    </joint>
    <link name="PendulumMass">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <geometry>
                <sphere radius="0.06"/>
            </geometry>
            <material name="">
                <color rgba="0.0 0.0 1.0 1.0"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="150.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>
</robot>
